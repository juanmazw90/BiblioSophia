{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for YouTube Insight",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:path:default",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-create",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "shell:allow-open",
    "store:default",
    "store:allow-load",
    "store:allow-set",
    "store:allow-get",
    "store:allow-delete",
    "store:allow-save",
    "store:allow-clear",
    "opener:default",
    "opener:allow-open-path",
    "opener:allow-open-url"
  ]
}
